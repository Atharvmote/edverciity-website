# Create .gitignore with these contents
echo "
# Dependencies
node_modules/
/.pnp
.pnp.js

# Production builds
/build/
/dist/

# Environment variables
.env
.env.local
.env.development.local
.env.production.local

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage
coverage/
*.lcov

# Cache
.cache/
.parcel-cache

# OS files
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/
*.swp
*.swo

# Large files
*.mp4
*.mov
*.avi
*.zip
*.rar
*.tar.gz
" > .gitignore